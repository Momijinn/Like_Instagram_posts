{
  "manifest_version": 3,
  "name": "Like Instagram posts",
  "description": "Like a post on your Instagram timeline.",
  "version": "1.0",
  "icons": {
    "16": "./icons/icon16.png",
    "32": "./icons/icon32.png",
    "48": "./icons/icon48.png",
    "128": "./icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.instagram.com/"
      ],
      "js": ["content-script.js"],
      "all_frames": true,
      "run_at": "document_end"
    }
  ]
}
